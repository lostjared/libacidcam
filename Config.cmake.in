@PACKAGE_INIT@

# Find required dependencies
include(CMakeFindDependencyMacro)
find_dependency(OpenCV REQUIRED COMPONENTS core imgproc imgcodecs highgui)
find_dependency(Threads REQUIRED)

# Import the targets
include("${CMAKE_CURRENT_LIST_DIR}/acidcamTargets.cmake")

# Define the include directories for backward compatibility
set_and_check(acidcam_INCLUDE_DIRS "@include_dest@")

# Define the library directories for backward compatibility
set_and_check(acidcam_LIBRARY_DIRS "@main_lib_dest@")

# Define the libraries for backward compatibility
set(acidcam_LIBRARIES acidcam::acidcam)
